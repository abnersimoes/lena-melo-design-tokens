{
  "name": "lena-melo-design-tokens",
  "version": "1.0.6",
  "description": "This example code is bare-bones to show you what this framework can do. If you have the style-dictionary module installed globally, you can `cd` into this directory and run: ```bash style-dictionary build ```",
  "main": "index.js",
  "scripts": {
    "build:assets": "mkdir build && cp -r tokens/assets build/assets",
    "build:static": "cp tokens/static/css/* build/css/",
    "build:tokens": "style-dictionary build",
    "build": "run-s clean build:assets build:tokens build:static",
    "clean": "rm -rf build",
    "test": "echo \"No test specified\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abnersimoes/lena-melo-design-tokens.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/abnersimoes/lena-melo-design-tokens/issues"
  },
  "homepage": "https://github.com/abnersimoes/lena-melo-design-tokens#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "style-dictionary": "^3.0.3"
  }
}
